@{
    ViewBag.Title = "楼宇管理";
}
@section head{
    <link href="../../../../Content/css/jquery-ui/base/jquery-ui.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Content/css/jquery-ui/datepicker/jquery-ui-1.8.11.datepicker.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Content/css/tab.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Content/css/form.css" rel="stylesheet" type="text/css" />
    <link href="../../../../Content/css/RoomSelect.css" rel="stylesheet" type="text/css" />

    <script src="../../../../Scripts/jquery.validate.min.js" type="text/javascript"></script>
    <script src="../../../../Scripts/jquery.tmpl.min.js" type="text/javascript"></script>
    <script src="../../../../Scripts/jquery-ui.min.js" type="text/javascript"></script>
    <script src="../../../../Scripts/jquery.ui.datepicker-zh-CN.js" type="text/javascript"></script>
    <script src="../../../../Scripts/jquery.timepicker.js" type="text/javascript"></script>
    <script src="../../../../Scripts/jquery.form.js" type="text/javascript"></script>
    <style type="text/css">
        #tabs .tab-div { padding-left:0;}
        .form-wrapper {border:1px #EAEAEA solid; height: auto; border-radius: 10px 10px 10px 10px;margin: 10px 10px; width: 710px; float:left; padding-bottom:10px;}
	    .form-wrapper h3{background-color: #BABABA;border-radius: 10px 10px 0px 0px; padding:3px;padding-left: 20px; display: block; color:White;}
	    .form-wrapper h3 img{ float:right; margin-right:10px; cursor:pointer;}
	    .form-wrapper table.input-table{ width:100%; margin:5px 0;}
	    .form-wrapper table.input-table th, .form-wrapper table.input-table td{ font-size: 12px;padding: 3px; border-left:1px #BABABA solid;border-right:1px #BABABA solid;}
	    .form-wrapper table.input-table th { border-top:1px #BABABA solid;}
	    .form-wrapper table.input-table td { border-bottom:1px #BABABA solid;}
	    .form-wrapper table.input-table td input { width: 80px;border:0; border-bottom:1px solid #BABABA; }
	    .form-wrapper table.input-table td input.small-width { width: 30px; }
	    .form-wrapper table.input-table td input.small2-width { width: 50px; }
	    /*
	    .em-form-ul li ul li span.multi-select-tip{ background: url("../../../../Content/images/img/icons/icon_down_arrow.png") no-repeat 120px 5px;border:1px solid #999999;border-radius: 3px 3px 0 0; display:block;width:140px; padding: 7px; text-align:center; cursor:pointer;}
	    .em-form-ul li ul li ol.multi-select {position: absolute; width:154px; border:1px solid #999999; border-top:0; border-radius: 0 0 3px 3px; margin:0; background-color:White; padding:0; display:none;}
	    .em-form-ul li ul li ol.multi-select li{overflow: hidden;height:20px;}
	    .em-form-ul li ul li ol.multi-select li input{ width:10px;}
	    */
	    .em-form-ul li.col1-width ul li label.error{ width: auto; float:none; margin-left:5px; display:inline;}
	    .time-picker{position: absolute; width:135px; border:1px solid #999999; margin:0; background-color:#BABABA; padding:2px; margin-top:2px;display:none;}
        .time-picker select{ width:50px; height:20px; padding:0; margin:0; float:left;}
        .time-picker a.confirm{background:url("../../../../Content/images/img/icons/icon_approve.png") no-repeat;float:left; width:16px; cursor:pointer; margin-top:3px;}
        .time-picker a.cancel{background:url("../../../../Content/images/img/icons/icon_cross_sml.png") no-repeat;float:left; width:16px; cursor:pointer; margin-top:3px;}
        .time-picker a:hover{ outline:1px #EAEAEA solid; }
	    .small-input-text{width: 30px;border:0; border-bottom:1px solid #BABABA;}
	    .a-plus-minus{border:1px #EAEAEA solid; text-decoration:none; padding:0 5px; margin-left:5px;}
	    .a-plus-minus:hover{ border-color: #BABABA;}
	    #errorTip { text-align:center; display:none; color:Red;}
	    hr{border:1px #EAEAEA solid;}
	    .em-form-ul li ul li .combox{position:relative;}
	    .em-form-ul li ul li .combox input{height:25px;position:absolute;left:0px; width:114px; border:0; margin-top:1px; margin-left:2px; padding-left:10px; padding-top:5px;}
	    .appendix-div{display:none;}
	    .appendix-div ul:after{content:"<li></li>"; visibility:hidden; display:block;}
	    .appendix-div ul li{margin:2px 0;border:1px #BABABA solid; width: 1050px;}
	    .appendix-div ul li:after{content:"."; visibility:hidden; display:block; height:0; clear:both;}
	    .appendix-div ul li label{float:left; width:40px; padding:5px; padding-top: 25px;text-align:center; margin:0;}
	    .appendix-div ul li label span{margin-left:-7px;margin-top:2px; display:block; width:50px;}
	    .appendix-div ul li table{float:right;width:1000px; margin:0;}
	    .appendix-div ul li table th, .appendix-div ul li table td{border-left:1px #BABABA solid;border-right:1px #BABABA solid;padding: 3px; text-align:center;white-space:nowrap;}
	    .appendix-div ul li table th{border-top:1px #BABABA solid;}
	    .appendix-div ul li table td{border-bottom:1px #BABABA solid;}
	    .appendix-div ul li table td input { width: 95%;border:0; background:none;}
	    .appendix-div ul li table td input.small { width: 40px;}
    </style>
}
<div class="contentcontainer">
<div class="headings altheading"><h2>楼宇管理</h2></div>
<div class="contentbox">
    <div id="tabs">
        <ul class="tab-ul">
		    <li><a href="@Url.Action("QueryBuilding", "Information")">查询楼宇</a></li>
		    <li class="current"><a>增加楼宇</a></li>
            <li><a href="@Url.Action("BuildingEnergyConsum", "Information")">楼宇额定能耗管理</a></li>
	    </ul>
	    <div class="tab-div">
        <div id="errorTip"><img src="@Url.Content("~/Content/images/img/icons/icon_error.png") " alt="Error" /> 部分数据输入有误，请重新输入!</div>
        <form id="myForm" action="@Url.Action("AddBuildingInfo", "Information")" method="post" enctype="multipart/form-data" >
        <div class="form-wrapper"><h3>必填信息</h3>
            <ul class="em-form-ul">
                <li>
                    <ul>
                        <li><span class="red">* </span>建筑名称：</li>
                        <li><input id="buildingName" name="b.BDI_Name" type="text" maxlength="100" class="inputbox" /></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li><span class="red">* </span>所属区域：</li>
                        <li><input id="areaName" name="areaName" type="text" class="inputbox" readonly="readonly"/></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
             </ul>
        </div>
        
        <div class="form-wrapper"><h3>基本信息<img src="../../../../Content/images/img/icons/icon_plus.png" /></h3>
            <ul class="em-form-ul hidden">
                <li>
                    <ul>
                        <li>建筑简介：</li>
                        <li><textarea id="introductionArea" name="b.BDI_Introduction" rows="1" cols="37" class="inputbox" style="overflow-y: hidden;" ></textarea></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>上传照片：</li>
                        <li><input name="uploadImg" type="file" class="inputbox" /></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li>
                    <ul>
                        <li>详细地址：</li>
                        <li><input name="b.BDI_Address" type="text" maxlength="100" class="inputbox" /></li>
                        <li></li>
                    </ul>
                    <ul style="width:163px;">
                        <li>所属院系：</li>
                        <li><input name="b.BDI_Academy" type="text" maxlength="100" class="inputbox" style="width:128px;" /></li>
                        <li></li>
                    </ul>
                    <ul style="width:163px;">
                        <li>楼主电话：</li>
                        <li><input name="b.BDI_Telephone" type="text" maxlength="100" class="inputbox" style="width:128px;" /></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li class="col4-width">
                    <ul>
                        <li>能耗统计负责人：</li>
                        <li><input name="b.BDI_LinkMan" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>能耗统计负责人电话：</li>
                        <li><input name="b.BDI_LinkManPhone" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>统计人员：</li>
                        <li><input name="b.BDI_StatMan" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>统计人员电话：</li>
                        <li><input name="b.BDI_StatManPhone" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li class="col4-width">
                    <ul>
                        <li>建筑类型：</li>
                        <li><div class="combox"><select >
                                 <option value=""></option> 
                                @foreach (var item in ViewBag.buildingTypesList)
                                    {                                                                                           
                                            <option value="@item.BT_ID">@item.BT_Name</option>                                      
                                    }
                             </select><input name="b.BDI_Type" class="hidden" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>建造年代：</li>
                        <li><input name="b.BDI_Date" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>建筑朝向：</li>
                        <li><input name="b.BDI_Direction" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>建筑使用人数：</li>
                        <li><input name="b.BDI_Users" type="text" maxlength="100" class="inputbox"/></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li><hr /></li>
                <li>
                    <ul>
                        <li>每天运行 <input name="b.BDI_RunPerWeek" type="text" maxlength="100" style="width:40px;" /> 小时：从 <input name="b.BDI_WeekStart" type="text" maxlength="100" style="width:65px;" /> 到 <input name="b.BDI_WeekEnd" type="text" maxlength="100" style="width:65px;" /> </li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>每周运行 <input name="b.BDI_RunPreMonth" type="text" maxlength="100" style="width:40px;" /> 天：从 <input name="b.BDI_MonthStart" type="text" maxlength="100" style="width:65px;" /> 到 <input name="b.BDI_MonthEnd" type="text" maxlength="100" style="width:65px;" /> </li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li>
                    <ul>
                        <li>每年运行 <input name="b.BDI_RunPerYear" type="text" maxlength="100" style="width:40px;" /> 月：&nbsp;&nbsp;&nbsp;从 <input name="b.BDI_YearStart" type="text" maxlength="100" style="width:65px;" /> 到 <input name="b.BDI_YearEnd" type="text" maxlength="100" style="width:65px;" /> </li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>假期：<input name="b.BDI_Holiday" type="text" maxlength="100" style="width:267px;" /></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li><hr /></li>
                <li class="col4-alt-width">
                    <ul>
                        <li>建筑高度：</li>
                        <li><input name="b.BDI_Height" type="text" maxlength="100" class="inputbox" /> m</li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>建造地上层数：</li>
                        <li><input name="b.BDI_FloorUp" type="text" maxlength="100" class="inputbox"/> 层</li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>建造地下层数：</li>
                        <li><input name="b.BDI_FloorDn" type="text" maxlength="100" class="inputbox"/> 层</li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>标准层层高：</li>
                        <li><input name="b.BDI_FloorHeight" type="text" maxlength="100" class="inputbox"/> m<sup>2</sup></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li><hr /></li>
                <li class="col4-alt-width">
                    <ul>
                        <li>建筑面积：</li>
                        <li><input name="b.BDI_Area" type="text" maxlength="100" class="inputbox" /> m<sup>2</sup></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>空调面积：</li>
                        <li><input name="b.BDI_AreaAir" type="text" maxlength="100" class="inputbox"/> m<sup>2</sup></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>采暖面积：</li>
                        <li><input name="b.BDI_AreaHot" type="text" maxlength="100" class="inputbox"/> m<sup>2</sup></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>特殊区域面积：</li>
                        <li><input name="b.BDI_AreaSpe" type="text" maxlength="100" class="inputbox" /> m<sup>2</sup></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li><hr /></li>
                <li class="col1-width" style="padding-left:10px;"><ul><li>以前是否进行过能源审计? (如果是，给出审计日期) <input type="radio" name="b.BDI_AauditFlag" checked="checked" value="0" />否<input type="radio" name="b.BDI_AauditFlag" value="1" />是 <span style="margin-left:20px;visibility:hidden;">审计日期：<input id="auditDate" name="b.BDI_AauditDate" type="text" maxlength="100" style="width:132px;" readonly="readonly" /></span></li><li></li></ul></li>
                <li class="col1-width" style="padding-left:10px;"><ul><li>以前是否实施过节能改造? (如果是，注明改造项目) <input type="radio" name="b.BDI_TransforFlag" checked="checked" value="0" />否<input type="radio" name="b.BDI_TransforFlag" value="1" />是 <span style="margin-left:20px;visibility:hidden;">改造项目：<input name="b.BDI_TransforContent" type="text" maxlength="100" style="width:132px;" /></span></li><li></li></ul></li>
                <li><hr /></li>
                <li>
                    <ul>
                        <li>电力公司名称：</li>
                        <li><input name="b.BDI_ElectricCompName" type="text" maxlength="100" class="inputbox" /></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>燃气供应公司名称：</li>
                        <li><input name="b.BDI_GasCompName" type="text" maxlength="100" class="inputbox" /></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
             </ul>
        </div>

        <div class="form-wrapper"><h3>建筑围护结构<img src="../../../../Content/images/img/icons/icon_plus.png" /></h3>
            <ul class="em-form-ul hidden">
                <li class="col4-width">
                    <ul>
                        <li>建筑结构形式：</li>
                        <li><div class="combox"><select><option value="砖混结构">砖混结构</option><option value="混凝土剪力墙">混凝土剪力墙</option><option value="钢结构">钢结构</option><option value="木结构">木结构</option><option value="玻璃幕墙">玻璃幕墙</option></select><input name="b.BDI_Structure" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>窗墙比：</li>
                        <li><input name="b.BDI_AreaRatio" type="text" maxlength="100" class="inputbox" style="width:105px;height:10px;"/> %</li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>外墙材料：</li>
                        <li><div class="combox"><select><option value="实心粘土墙">实心粘土墙</option><option value="空心粘土砖(多孔)">空心粘土砖(多孔)</option><option value="灰砂砖">灰砂砖</option><option value="加气混凝土砌块">加气混凝土砌块</option><option value="混凝土小型空心砌块(多孔)">混凝土小型空心砌块(多孔)</option></select><input name="b.BDI_KindsOut" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>是否保温：</li>
                        <li>
                            <select name="b.BDI_WarmKeep"><optgroup label="否"><option value="否,无保温">无保温</option></optgroup><optgroup label="是"><option value="是,内保温">内保温</option><option value="是,外保温">外保温</option></optgroup></select>
                        </li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li class="col4-width">
                    <ul>
                        <li>外窗类型：</li>
                        <li><div class="combox"><select><option value="单玻单层窗">单玻单层窗</option><option value="单玻双层窗">单玻双层窗</option><option value="单玻单层窗+单玻双层窗">单玻单层窗+单玻双层窗</option><option value="中空双层玻璃窗">中空双层玻璃窗</option><option value="中空三层玻璃窗">中空三层玻璃窗</option><option value="中空充惰性气体">中空充惰性气体</option></select><input name="b.BDI_WinOut" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>有无遮阳：</li>
                        <li>
                            <select name="b.BDI_Visor"><optgroup label="否"><option value="否,无遮阳">无遮阳</option></optgroup><optgroup label="是"><option value="是,固定外遮阳">固定外遮阳</option><option value="是,固定内遮阳">固定内遮阳</option></optgroup></select>
                        </li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>玻璃类型：</li>
                        <li><div class="combox"><select><option value="普通玻璃">普通玻璃</option><option value="镀膜玻璃">镀膜玻璃</option><option value="Low-e玻璃">Low-e玻璃</option></select><input name="b.BDI_Glass" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>窗框材料：</li>
                        <li><div class="combox"><select><option value="钢窗">钢窗</option><option value="铝合金">铝合金</option><option value="木窗">木窗</option><option value="断热窗框">断热窗框</option></select><input name="b.BDI_Win" maxlength="100" /></div></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
             </ul>
        </div>

        <div class="form-wrapper"><h3>空调系统<img src="../../../../Content/images/img/icons/icon_plus.png" /></h3>
            <ul class="em-form-ul hidden">
                <li class="col3-alt-width">
                    <ul>
                        <li>设定温度供冷温度： 供热温度：</li>
                        <li>
                        <input name="b.BDI_TemCool" type="text" maxlength="100" class="inputbox" style="width:40px; margin-left:40px;" /> <sup>o</sup>C
                        <input name="b.BDI_TemHot" type="text" maxlength="100" class="inputbox" style="width:40px;" /> <sup>o</sup>C
                        </li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>特殊区域供冷温度： 供热温度：</li>
                        <li>
                        <input name="b.BDI_TemSepcialCool" type="text" maxlength="100" class="inputbox" style="width:40px; margin-left:40px;" /> <sup>o</sup>C
                        <input name="b.BDI_TemSepcialHot" type="text" maxlength="100" class="inputbox" style="width:40px;" /> <sup>o</sup>C
                        </li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>空调系统形式：</li>
                        <li><div class="combox"><select id="airConditionSelect" ><option value=""></option><option value="集中式全空气系统">集中式全空气系统</option><option value="风机盘管＋新风系统">风机盘管＋新风系统</option><option value="分体式空调或VRV的局部式机组系统">分体式空调或VRV的局部式机组系统</option></select><input name="b.BDI_AirSys" maxlength="100" style="width:124px;" /></div></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
             </ul>
        </div>
        <div class="form-wrapper"><h3>冷热源设备<img src="../../../../Content/images/img/icons/icon_plus.png" /></h3>
            <ul class="em-form-ul hidden">
                <li class="col3-width">
                    <ul>
                        <li>水冷：</li>
                        <li><select id="devCoolSelect" name="b.BDI_DevCool"><option value=""></option><option value="水冷式机组（离心式、螺杆式等）">水冷式机组（离心式、螺杆式等）</option></select></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>热泵：</li>
                        <li><select id="devHotSelect" name="b.BDI_DevHot"><option value=""></option><option value="空气源热泵机组">空气源热泵机组</option><option value="水源热泵机组">水源热泵机组</option><option value="地源（土壤源）热泵机组">地源（土壤源）热泵机组</option><option value="水环热泵机组">水环热泵机组</option></select></li>
                        <li></li>
                    </ul>
                    <ul>
                        <li>溴化锂吸收式：</li>
                        <li><select id="liBrSelect" name="b.BDI_LiBr"><option value=""></option><option value="直燃型溴化锂吸收式机组">直燃型溴化锂吸收式机组</option><option value="蒸汽型溴化锂吸收式机组">蒸汽型溴化锂吸收式机组</option><option value="热水型溴化锂吸收式机组">热水型溴化锂吸收式机组</option></select></li>
                        <li></li>
                    </ul>
                    <div class="clear"></div>
                </li>
                <li class="col3-width">
                    <ul>
                        <li>分体式空调：</li>
                        <li><select id="airSplitSelect" name="b.BDI_AirSplit"><option value=""></option><option value="分体式空调或VRV的局部式机组">分体式空调或VRV的局部式机组</option></select></li>
                        <li></li>
