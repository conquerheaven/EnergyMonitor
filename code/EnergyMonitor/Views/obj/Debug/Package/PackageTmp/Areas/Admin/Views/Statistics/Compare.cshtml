        //                            $("#powerType").val(tempIDs);
        //                            if (checkedObjs.length > 1) {
        //                                $("#powerTypeName").val(tempName + " ... （" + checkedObjs.length + "）");
        //                            } else {
        //                                $("#powerTypeName").val(tempName);
        //                            }
        //                        } else {
        //                            $("#treeDialogErrorTip").slideDown("fast");
